{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "AmyDataFactory-Dev"
		},
		"DataLakeConnect_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'DataLakeConnect'"
		},
		"DevStorageAcct_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'DevStorageAcct'"
		},
		"GithubAmy_credential": {
			"type": "secureString",
			"metadata": "Secure string for 'credential' of 'GithubAmy'"
		},
		"DataLakeConnect_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://amystoragelakedev.dfs.core.windows.net"
		},
		"GithubAmy_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "afelthe"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "AmyDataFactory-Dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"DataLakeConnect_accountKey": {
						"value": "[parameters('DataLakeConnect_accountKey')]"
					},
					"DevStorageAcct_connectionString": {
						"value": "[parameters('DevStorageAcct_connectionString')]"
					},
					"GithubAmy_credential": {
						"value": "[parameters('GithubAmy_credential')]"
					},
					"DataLakeConnect_properties_typeProperties_url": {
						"value": "[parameters('DataLakeConnect_properties_typeProperties_url')]"
					},
					"GithubAmy_properties_typeProperties_username": {
						"value": "[parameters('GithubAmy_properties_typeProperties_username')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}